#!/usr/bin/env ruby
require 'yaml'
require 'uri'
rc = File.expand_path("~/.hurlrc")
config = YAML.load_file(rc)
command = ARGV[0]
case command
when /.*/
  puts "admin command line script not functional at this time"
  exit 1
end
=begin
cmd = "curl --insecure -X POST -H 'Accept: text/plain' #{data} -u #{config['user']}:#{config['password']} #{config['app']}"
puts "running:\n#{cmd}"
result = `#{cmd}`
puts result
=end
